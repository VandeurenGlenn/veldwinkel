{
  "name": "veldwinkel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "rollup -c",
    "watch": "set production=false && rollup -c -w",
    "precache": "sw-precache --config=sw-precache-config.js && sw-precache --config=sw-precache-config-admin.js && sw-precache --config=sw-precache-config-shop.js",
    "build": "set production=true && npm run compile",
    "serve": "jsproject --serve www",
    "serve:shop": "jsproject --serve www/shop --port 3001",
    "serve:admin": "jsproject --serve www/admin --port 3002",
    "serve:all": "npm-run-parallel serve serve:shop serve:admin",
    "start": "npm-run-parallel serve watch ",
    "docs": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandeurenglenn/veldwinkel.git"
  },
  "keywords": [
    "store"
  ],
  "author": "Glenn Vandeuren <vandeurenglenn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vandeurenglenn/veldwinkel/issues"
  },
  "homepage": "https://github.com/vandeurenglenn/veldwinkel#readme",
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "classProperties": true,
          "objectRestSpread": true,
          "doExpressions": true,
          "functionBind": true,
          "functionSent": true,
          "asyncGenerators": true,
          "decorators": true,
          "exportExtensions": true,
          "dynamicImport": true
        }
      }
    ]
  },
  "dependencies": {
    "@tensorflow-models/mobilenet": "^1.0.1",
    "@tensorflow/tfjs": "^1.2.9",
    "@vandeurenglenn/custom-date": "^1.6.0",
    "backed": "^0.13.0",
    "custom-drawer": "^1.0.0",
    "custom-input": "git+https://github.com/AndrewVanardennen/custom-input.git",
    "custom-pages": "^0.12.0",
    "custom-renderer-mixin": "^1.1.2",
    "custom-select-mixins": "^0.13.0",
    "custom-selector": "^0.2.0",
    "custom-svg-icon": "^0.10.2",
    "custom-svg-iconset": "^0.9.2",
    "custom-tabs": "^0.11.0",
    "custom-template-if": "^1.0.0",
    "idb-keyval": "^3.2.0",
    "lfc-api": "^2.18.2",
    "nodemailer": "^6.3.0",
    "pica": "^5.1.0",
    "raw-body": "^2.4.1",
    "typed.js": "^2.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@vandeurenglenn/little-pubsub": "^1.2.3",
    "acorn-class-fields": "^0.3.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-html": "^5.0.5",
    "npm-run-parallel": "^0.6.0",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "sw-precache": "^5.2.1"
  }
}
